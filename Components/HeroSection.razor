				<div class="Content flex-col  justify-start items-center gap-8 flex">
					<figure class="PhotoName flex-col justify-start items-center gap-4 flex">
						<img class="ProfilePhoto rounded-3xl border-2 border-cyan-400 h-32" src="./profilePic.jpeg" />
						<figcaption class="Name flex-col justify-start items-center flex">
							<h3
								class="Sinan text-white text-3xl font-medium font-['IBM Plex Mono'] capitalize leading-10">
								@Profile?.Name</h3>
							<div
								class="FullStackDeveloper text-white text-sm font-normal font-['IBM Plex Mono'] leading-none">
								@Profile?.Profession</div>
						</figcaption>
					</figure>

					<div class=" flex-col justify-start items-start gap-4 flex">
						<address class="email flex justify-center items-center gap-4">
							<span class="material-symbols-outlined text-sm text-white">
								email</span>
							<a href="mailto:abdurrahman_sinan@hotmail.com"
								class="email-link text-white text-sm  leading-none">
								@Email
							</a>
						</address>
						<div class="location flex justify-center items-center gap-4">
							<span class="material-symbols-outlined text-sm text-white">
								location_on</span>
							<p class="location-name text-white text-sm leading-none">
								@Profile?.Location
							</p>
						</div>
						<div class="freelancer flex justify-center items-center gap-4">
							<span class="material-symbols-outlined text-sm text-white">
								work</span>
							<p class="freelancer-status text-white text-sm font-normal font-mono leading-none">
								Full-time / Freelancer
							</p>
						</div>
					</div>
					<div class="Label justify-start items-start gap-4 inline-flex">
						<div class="Html px-2 bg-cyan-400 rounded-lg justify-center items-center gap-4 flex">
							<div class="Html text-gray-800 text-sm font-normal font-['IBM Plex Mono'] leading-none">
								HTML</div>
						</div>
						<div class="Css px-2 bg-cyan-400 rounded-lg justify-center items-center gap-4 flex">
							<div class="Css text-gray-800 text-sm font-normal font-['IBM Plex Mono'] leading-none">
								CSS</div>
						</div>
						<div class="Js px-2 bg-cyan-400 rounded-lg justify-center items-center gap-4 flex">
							<div class="Js text-gray-800 text-sm font-normal font-['IBM Plex Mono'] leading-none">JS
							</div>
						</div>
						<div class="React px-2 bg-cyan-400 rounded-lg justify-center items-center gap-4 flex">
							<div class="React text-gray-800 text-sm font-normal font-['IBM Plex Mono'] leading-none">
								REACT</div>
						</div>
					</div>
				</div>

				<div class="flex justify-center align-middle mt-4">
					<a href="@Profile?.CvLink" target="_blank"
						class="text-md font-normal font-['Ubuntu'] capitalize leading-normal bg-white px-3 py-1 rounded-xl hover:cursor-pointer">
						Download CV <span class="material-symbols-outlined text-xl  ">download</span></a>
				</div>
			</div>
			<div class="flex flex-col xl:flex-row xl:justify-start gap-32 justify-center ">
				<div class="Info justify-start items-start gap-8 flex flex-col">
					<div class="Title flex-col flex">
						<div class="H1 text-teal-200 text-sm font-normal font-['IBM Plex Mono'] leading-none pb-5">
							&lt;h1&gt;
						</div>
						<div class="Text self-stretch px-4 text-white text-3xl md:text-5xl">
